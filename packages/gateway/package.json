{
  "name": "cfc-gateway",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rmdir /s /q dist 2>nul || exit 0",
    "build:clean": "npm run clean && npm run build"
  },
  "dependencies": {
    "@types/express-rate-limit": "^5.1.3",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.0.0",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.8.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}